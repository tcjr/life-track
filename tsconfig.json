{
  "extends": "@ember/app-tsconfig",
  "include": ["app", "tests", "types"],
  "compilerOptions": {
    "allowJs": true,
    "paths": {
      "#app/*": ["./app/*"],
      "#components/*": ["./app/components/*"],
      "#models/*": ["./app/models/*"],
      "#resources/*": ["./app/resources/*"],
      "#services/*": ["./app/services/*"],
      "#tests/*": ["./tests/*"],
      "*": ["./types/*"]
    },
    "types": [
      "ember-source/types",
      "@embroider/core/virtual",
      "vite/client",
      "@glint/ember-tsc/types"
    ]
  }
}
